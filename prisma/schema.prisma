  compra_aprazo                                          Decimal?                @db.Decimal
  razao_social                                           String?
  fone                                                   String?
  email                                                  String?
  desconto                                               Decimal?                @db.Decimal
  foto                                                   String?
  bandeira                                               String?
  empresa                                                String?
  tipo_pagamento                                         String?
  carrinho_produto_carrinho_produto_dar_baixaTodominio   carrinho_produto[]      @relation("carrinho_produto_dar_baixaTodominio")
  carrinho_produto_carrinho_produto_salvoTodominio       carrinho_produto[]      @relation("carrinho_produto_salvoTodominio")
  categoria_categoria_deletadoTodominio                  categoria[]             @relation("categoria_deletadoTodominio")
  categoria_categoria_destaqueTodominio                  categoria[]             @relation("categoria_destaqueTodominio")
  complemento_item                                       complemento_item[]
  complemento_tipo_complemento_tipo_deletadoTodominio    complemento_tipo[]      @relation("complemento_tipo_deletadoTodominio")
  complemento_tipo_complemento_tipo_obrigatorioTodominio complemento_tipo[]      @relation("complemento_tipo_obrigatorioTodominio")
  cupom                                                  cupom[]
  endereco                                               endereco[]
  estabelecimento_estabelecimento_bannerTodominio        estabelecimento[]       @relation("estabelecimento_bannerTodominio")
  estabelecimento_estabelecimento_chatTodominio          estabelecimento[]       @relation("estabelecimento_chatTodominio")
  estabelecimento_estabelecimento_status_planoTodominio  estabelecimento[]       @relation("estabelecimento_status_planoTodominio")
  horario_funcionamento                                  horario_funcionamento[]
  lista_forma_pagamento                                  lista_forma_pagamento[]
  pedido_pedido_forma_pagamentoTodominio                 pedido[]                @relation("pedido_forma_pagamentoTodominio")
  pedido_pedido_pedido_finalizadoTodominio               pedido[]                @relation("pedido_pedido_finalizadoTodominio")
  plano                                                  plano[]
  produto_produto_deletadoTodominio                      produto[]               @relation("produto_deletadoTodominio")
  produto_produto_destaqueTodominio                      produto[]               @relation("produto_destaqueTodominio")
  produto_produto_grupoTodominio                         produto[]               @relation("produto_grupoTodominio")
  produto_produto_uni_medidaTodominio                    produto[]               @relation("produto_uni_medidaTodominio")
  receber_pagar_receber_pagar_plano_contasTodominio      receber_pagar[]         @relation("receber_pagar_plano_contasTodominio")
  receber_pagar_receber_pagar_tipoTodominio              receber_pagar[]         @relation("receber_pagar_tipoTodominio")
  usuario_usuario_adminTodominio                         usuario[]               @relation("usuario_adminTodominio")
  usuario_usuario_perfilTodominio                        usuario[]               @relation("usuario_perfilTodominio")
  usuario_usuario_profissaoTodominio                     usuario[]               @relation("usuario_profissaoTodominio")
}

model endereco {
  id_endereco                      String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  lograduro                        String?
  num                              String?
  complemento                      String?
  bairro                           String?
