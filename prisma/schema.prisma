  pedido_pedido_estabelecimentoToestabelecimento                               pedido[]                @relation("pedido_estabelecimentoToestabelecimento")
  receber_pagar_receber_pagar_estabelecimentoToestabelecimento                 receber_pagar[]         @relation("receber_pagar_estabelecimentoToestabelecimento")
  usuario_usuario_estabelecimentoToestabelecimento                             usuario[]               @relation("usuario_estabelecimentoToestabelecimento")
