generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alerta {
  id_alerta                                               String           @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  mensagem                                                String?
  estabelecimento                                         String?          @db.Uuid
  estabelecimento_alerta_estabelecimentoToestabelecimento estabelecimento? @relation("alerta_estabelecimentoToestabelecimento", fields: [estabelecimento], references: [id_estabelecimento], onDelete: NoAction)
}

model area_entrega {
  id_area_entrega                                               String           @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  distancia_maxima                                              Decimal?         @db.Decimal
  distancia_minima                                              Decimal?         @db.Decimal
  valor                                                         Decimal?         @db.Decimal
  estabelecimento                                               String?          @db.Uuid
  estabelecimento_area_entrega_estabelecimentoToestabelecimento estabelecimento? @relation("area_entrega_estabelecimentoToestabelecimento", fields: [estabelecimento], references: [id_estabelecimento], onDelete: NoAction)
}

model aviso {
  id_aviso                                               String           @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  mensagem                                               String?
  estabelecimento                                        String?          @db.Uuid
  tipo                                                   String?
  status                                                 String?
  estabelecimento_aviso_estabelecimentoToestabelecimento estabelecimento? @relation("aviso_estabelecimentoToestabelecimento", fields: [estabelecimento], references: [id_estabelecimento], onDelete: NoAction)
}

model carrinho_complemento {
  id_carrinho_complemento                                                                          String                       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  carrinho_produto                                                                                 String?                      @db.Uuid
  complemento_item                                                                                 String?                      @db.Uuid
  complemento_tipo                                                                                 String?                      @db.Uuid
  quantidade                                                                                       BigInt?
  valor                                                                                            Decimal?                     @db.Decimal
  carrinho_produto_carrinho_complemento_carrinho_produtoTocarrinho_produto                         carrinho_produto?            @relation("carrinho_complemento_carrinho_produtoTocarrinho_produto", fields: [carrinho_produto], references: [id_carrinho_produto], onDelete: NoAction)
  complemento_item_carrinho_complemento_complemento_itemTocomplemento_item                         complemento_item?            @relation("carrinho_complemento_complemento_itemTocomplemento_item", fields: [complemento_item], references: [id_complemento_item], onDelete: NoAction)
  complemento_tipo_carrinho_complemento_complemento_tipoTocomplemento_tipo                         complemento_tipo?            @relation("carrinho_complemento_complemento_tipoTocomplemento_tipo", fields: [complemento_tipo], references: [id_complemento_tipo], onDelete: NoAction)
  lista_carrinho_complemento_lista_carrinho_complemento_carrinho_complementoTocarrinho_complemento lista_carrinho_complemento[] @relation("lista_carrinho_complemento_carrinho_complementoTocarrinho_complemento")
}

model carrinho_produto {
  id_carrinho_produto                                                                      String                       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  atendente                                                                                String?                      @db.Uuid
  obs                                                                                      String?
  produto                                                                                  String?                      @db.Uuid
  quantidade                                                                               BigInt?
  valor_produto                                                                            Decimal?                     @db.Decimal
  valor_total                                                                              Decimal?                     @db.Decimal
  dar_baixa                                                                                String?                      @db.Uuid
  salvo                                                                                    String?                      @db.Uuid
  carrinho_complemento_carrinho_complemento_carrinho_produtoTocarrinho_produto             carrinho_complemento[]       @relation("carrinho_complemento_carrinho_produtoTocarrinho_produto")
  usuario                                                                                  usuario?                     @relation(fields: [atendente], references: [id_usuario], onDelete: NoAction)
  dominio_carrinho_produto_dar_baixaTodominio                                              dominio?                     @relation("carrinho_produto_dar_baixaTodominio", fields: [dar_baixa], references: [id_dominio], onDelete: NoAction)
  produto_carrinho_produto_produtoToproduto                                                produto?                     @relation("carrinho_produto_produtoToproduto", fields: [produto], references: [id_produto], onDelete: NoAction)
  dominio_carrinho_produto_salvoTodominio                                                  dominio?                     @relation("carrinho_produto_salvoTodominio", fields: [salvo], references: [id_dominio], onDelete: NoAction)
  lista_carrinho_complemento_lista_carrinho_complemento_carrinho_produtoTocarrinho_produto lista_carrinho_complemento[] @relation("lista_carrinho_complemento_carrinho_produtoTocarrinho_produto")
  lista_carrinho_produto_lista_carrinho_produto_carrinho_produtoTocarrinho_produto         lista_carrinho_produto[]     @relation("lista_carrinho_produto_carrinho_produtoTocarrinho_produto")
}

model categoria {
  id_categoria                                               String           @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  deletado                                                   String?          @db.Uuid
  destaque                                                   String?          @db.Uuid
  estabelecimento                                            String?          @db.Uuid
  nome                                                       String?
  tipo_categoria                                             String?          @db.Uuid
  dominio_categoria_deletadoTodominio                        dominio?         @relation("categoria_deletadoTodominio", fields: [deletado], references: [id_dominio], onDelete: NoAction)
  dominio_categoria_destaqueTodominio                        dominio?         @relation("categoria_destaqueTodominio", fields: [destaque], references: [id_dominio], onDelete: NoAction)
  estabelecimento_categoria_estabelecimentoToestabelecimento estabelecimento? @relation("categoria_estabelecimentoToestabelecimento", fields: [estabelecimento], references: [id_estabelecimento], onDelete: NoAction)
  tipo_categoria_categoria_tipo_categoriaTotipo_categoria    tipo_categoria?  @relation("categoria_tipo_categoriaTotipo_categoria", fields: [tipo_categoria], references: [id_tipo_categoria], onDelete: NoAction)
  produto_produto_categoriaTocategoria                       produto[]        @relation("produto_categoriaTocategoria")
}

model complemento_item {
  id_complemento_item                                                              String                   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  complemento_tipo                                                                 String?                  @db.Uuid
  deletado                                                                         String?                  @db.Uuid
  descricao                                                                        String?
  nome                                                                             String?
  valor                                                                            Decimal?                 @db.Decimal
  carrinho_complemento_carrinho_complemento_complemento_itemTocomplemento_item     carrinho_complemento[]   @relation("carrinho_complemento_complemento_itemTocomplemento_item")
  complemento_tipo_complemento_item_complemento_tipoTocomplemento_tipo             complemento_tipo?        @relation("complemento_item_complemento_tipoTocomplemento_tipo", fields: [complemento_tipo], references: [id_complemento_tipo], onDelete: NoAction)
  dominio                                                                          dominio?                 @relation(fields: [deletado], references: [id_dominio], onDelete: NoAction)
  lista_complemento_item_lista_complemento_item_complemento_itemTocomplemento_item lista_complemento_item[] @relation("lista_complemento_item_complemento_itemTocomplemento_item")
}

model complemento_tipo {
  id_complemento_tipo                                                                     String                   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  deletado                                                                                String?                  @db.Uuid
  lista_complemento_itens                                                                 String?                  @db.Uuid
  nome                                                                                    String?
  obrigatorio                                                                             String?                  @db.Uuid
  produto                                                                                 String?                  @db.Uuid
  quantidade_maxima                                                                       BigInt?
  quantidade_minima                                                                       BigInt?
  carrinho_complemento_carrinho_complemento_complemento_tipoTocomplemento_tipo            carrinho_complemento[]   @relation("carrinho_complemento_complemento_tipoTocomplemento_tipo")
  complemento_item_complemento_item_complemento_tipoTocomplemento_tipo                    complemento_item[]       @relation("complemento_item_complemento_tipoTocomplemento_tipo")
  dominio_complemento_tipo_deletadoTodominio                                              dominio?                 @relation("complemento_tipo_deletadoTodominio", fields: [deletado], references: [id_dominio], onDelete: NoAction)
  lista_complemento_item_complemento_tipo_lista_complemento_itensTolista_complemento_item lista_complemento_item?  @relation("complemento_tipo_lista_complemento_itensTolista_complemento_item", fields: [lista_complemento_itens], references: [id_lista_complemento_item], onDelete: NoAction)
  dominio_complemento_tipo_obrigatorioTodominio                                           dominio?                 @relation("complemento_tipo_obrigatorioTodominio", fields: [obrigatorio], references: [id_dominio], onDelete: NoAction)
  produto_complemento_tipo_produtoToproduto                                               produto?                 @relation("complemento_tipo_produtoToproduto", fields: [produto], references: [id_produto], onDelete: NoAction)
  lista_complemento_item_lista_complemento_item_complemento_tipoTocomplemento_tipo        lista_complemento_item[] @relation("lista_complemento_item_complemento_tipoTocomplemento_tipo")
  lista_complemento_tipo_lista_complemento_tipo_complemento_tipoTocomplemento_tipo        lista_complemento_tipo[] @relation("lista_complemento_tipo_complemento_tipoTocomplemento_tipo")
}

model cupom {
  id_cupom                                               String           @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  desconto                                               BigInt?
  descricao                                              String?
  estabelecimento                                        String?          @db.Uuid
  tipo_cupom                                             String?          @db.Uuid
  titulo                                                 String?
  estabelecimento_cupom_estabelecimentoToestabelecimento estabelecimento? @relation("cupom_estabelecimentoToestabelecimento", fields: [estabelecimento], references: [id_estabelecimento], onDelete: NoAction)
  dominio                                                dominio?         @relation(fields: [tipo_cupom], references: [id_dominio], onDelete: NoAction)
}

