  cupom_cupom_estabelecimentoToestabelecimento                                 cupom[]                 @relation("cupom_estabelecimentoToestabelecimento")
  dominio_estabelecimento_bannerTodominio                                      dominio?                @relation("estabelecimento_bannerTodominio", fields: [banner], references: [id_dominio], onDelete: NoAction)
  dominio_estabelecimento_chatTodominio                                        dominio?                @relation("estabelecimento_chatTodominio", fields: [chat], references: [id_dominio], onDelete: NoAction)
  plano_estabelecimento_planoToplano                                           plano?                  @relation("estabelecimento_planoToplano", fields: [plano], references: [id_plano], onDelete: NoAction)
  usuario_estabelecimento_responsavelTousuario                                 usuario?                @relation("estabelecimento_responsavelTousuario", fields: [responsavel], references: [id_usuario], onDelete: NoAction)
